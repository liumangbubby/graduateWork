<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="ref">
	<typeAlias alias="errorexamtem" type="cn.gov.jyw.pojo.error_exam_tem" />
	<resultMap id="errortemresultMap" class="errorexamtem">
		<result property="user_id" column="user_id" />
		<result property="exam_id" column="exam_id" />
	</resultMap>
	
	<select id="QueryTemErrorInfoByUser" parameterClass="int" resultMap="errortemresultMap">
		select user_id,exam_id from error_exam_tem er where er.user_id = #userid#
	</select>
	<insert id="InsertErrorTem" parameterClass="errorexamtem">
		insert into
		error_exam_tem(user_id,exam_id) values(#user_id#,#exam_id#)
	</insert>
	<delete id="DeleteAll">
		delete from error_exam_tem
	</delete>
</sqlMap>